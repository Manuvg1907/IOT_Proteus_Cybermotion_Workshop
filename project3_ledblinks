        LED2.on();
        delay(100);
        LED2.off();
        delay(100);
        LED3.on();
        delay(100);
        LED3.off();
        delay(100);
        LED4.on();
        delay(100);
        LED4.off();
        delay(100);
        LED5.on();
        delay(100);
        LED5.off();
        delay(100);
        nextInstruction = 0;
        break;
    }
  }
}

